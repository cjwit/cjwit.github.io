osascript -e 'tell app "Terminal"
    do script "cd /Users/cwituls/Documents/cjwit.github.io/covid"
end tell'
osascript -e 'tell app "Terminal"
    do script "cd /Users/cwituls/Documents/cjwit.github.io/covid && python -m SimpleHTTPServer"
end tell'
curl "https://raw.githubusercontent.com/nytimes/covid-19-data/master/us-states.csv" -o "/Users/cwituls/Documents/cjwit.github.io/covid/covid.csv"
cd /Users/cwituls/Documents/cjwit.github.io/covid && python covid.py
osascript -e 'tell app "Terminal"
    do script "cd /Users/cwituls/Documents/cjwit.github.io/covid && git commit -am 'automated update' && git push origin master"
end tell'
